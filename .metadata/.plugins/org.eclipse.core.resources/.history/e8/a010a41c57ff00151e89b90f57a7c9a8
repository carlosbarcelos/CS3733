package common.entities;
import java.awt.Color;

public class Square {

	final int WIDTH = 20;
	final int HEIGHT = 20;
	Color c;
	Coordinate points;
	String type; // Options: null, filled, empty, number, hint
	
	public Square(Color c, Coordinate points, String type){
		this.c = c;
		this.points = points;
		this.type = type;
	}
}
